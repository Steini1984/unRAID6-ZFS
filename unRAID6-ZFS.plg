<!-- Install package based on kernel  -->
<FILE Run="/bin/bash">
<INLINE>
# source unRAID version
. /etc/unraid-version

# kernel version
kernel_version=$(uname -a)

download_install() {
  local dest="&plugin;/packages/${1}"
  local src="&repo;/packages/${1}"
 
  mkdir -p "&plugin;/packages"
  if [ ! -f "${dest}" ]; then
    curl --location --silent --fail "${src}" --output "${dest}"
  fi
  if [ ! -f "${dest}.md5" ]; then
    curl --location --silent --fail "${src}.md5" --output "${dest}.md5"
  fi

  orig_md5=$(/usr/bin/cat $dest.md5)
  file_md5=$(/usr/bin/md5sum ${dest})

  if [ "${file_md5:0:32}" != "${orig_md5:0:32}" ]; then
    echo "Wrong '${1}' package md5 hash."
    rm "${dest}"
    exit 1
  else
    /sbin/installpkg "$dest"
    #delte old tgz
    find "&plugin;/packages/"* ! -name *"$version"* -exec rm {} \;
  fi
}

if [[ $kernel_version == *"4.18.19-unRAID"* ]]; then #6.6.1
  spl_pkg="spl-0.7.11-unRAID-6.6.1.x86_64.tgz"
  zfs_pkg="zfs-0.7.11-unRAID-6.6.1.x86_64.tgz"
  supported_kernel=true

elif [[ $kernel_version == *"4.18.8-unRAID"* ]]; then #6.6.0
  spl_pkg="spl-0.7.11-unRAID-6.6.0.x86_64.tgz"
  zfs_pkg="zfs-0.7.11-unRAID-6.6.0.x86_64.tgz"
  supported_kernel=true

elif [[ $kernel_version == *"4.14.49-unRAID"* ]]; then #6.5.3
  spl_pkg="spl-0.7.9-unRAID-6.5.3.x86_64.tgz"
  zfs_pkg="zfs-0.7.9-unRAID-6.5.3.x86_64.tgz"
  supported_kernel=true

elif [[ $kernel_version == *"4.14.40-unRAID"* ]]; then #6.5.2
  spl_pkg="spl-0.7.9-unRAID-6.5.2.x86_64.tgz"
  zfs_pkg="zfs-0.7.9-unRAID-6.5.2.x86_64.tgz"
  supported_kernel=true

elif [[ $kernel_version == *"4.14.35-unRAID"* ]]; then #6.5.1
  spl_pkg="spl-0.7.8-unRAID-6.5.1.x86_64.tgz"
  zfs_pkg="zfs-0.7.8-unRAID-6.5.1.x86_64.tgz"
  supported_kernel=true

elif [[ $kernel_version == *"4.14.26-unRAID"* ]]; then #6.5.0
  spl_pkg="spl-0.7.6-unRAID-6.5.0.x86_64.tgz"
  zfs_pkg="zfs-0.7.6-unRAID-6.5.0.x86_64.tgz"
  supported_kernel=true

elif [[ $kernel_version == *"4.14.16-unRAID"* ]]; then #6.4.1
  spl_pkg="spl-0.7.6-unRAID-6.4.1.x86_64.tgz"
  zfs_pkg="zfs-0.7.6-unRAID-6.4.1.x86_64.tgz"
  supported_kernel=true

elif [[ $kernel_version == *"4.14.13-unRAID"* ]]; then #6.4.0
  spl_pkg="spl-0.7.6-unRAID-6.4.0.x86_64.tgz"
  zfs_pkg="zfs-0.7.6-unRAID-6.4.0.x86_64.tgz"
  supported_kernel=true

elif [[ $kernel_version == *"4.9.30-unRAID"* ]]; then #6.3.5
  spl_pkg="spl-0.7.2-unRAID6.3.5.x86_64.tgz"
  zfs_pkg="zfs-0.7.2-unRAID6.3.5.x86_64.tgz"
  supported_kernel=true

elif [[ $kernel_version == *"4.9.28-unRAID"* ]]; then #6.3.4
  spl_pkg="spl-0.6.5.9-unRAID6.3.4.x86_64.tgz"
  zfs_pkg="zfs-0.6.5.9-unRAID6.3.4.x86_64.tgz"
  supported_kernel=true

elif [[ $kernel_version == *"4.9.19-unRAID"* ]]; then #6.3.3
  spl_pkg="spl-0.6.5.9-unRAID6.3.3.x86_64.tgz"
  zfs_pkg="zfs-0.6.5.9-unRAID6.3.3.x86_64.tgz"
  supported_kernel=true

else
  echo ""
  echo "Unsupported kernel detected!" 
  echo ""
  /usr/local/emhttp/plugins/dynamix/scripts/notify -e "ZFS not installed!" -s "Unsupported kernel detected!" -d "Supported versions are: unRAID 6.3.3 - 6.6.1" -i "alert"
  exit 1
fi

if [ "$supported_kernel" = true ] ; then
  if mount | grep /lib/modules > /dev/null; 
  then
        echo "Remounting modules"
        cp -r /lib/modules /tmp
        umount -l /lib/modules/
        rm -rf  /lib/modules
        mv -f  /tmp/modules /lib
  fi

  download_install "${spl_pkg}"
  download_install "${zfs_pkg}"

  echo "Loading ZFS modules and importing pools (this could take some time....)"
  depmod >> /dev/null
  modprobe zfs  >> /dev/null
  zpool import -a >> /dev/null
  #fix for the icon
  chmod 755 /usr/local/emhttp/plugins/unRAID6-ZFS/
fi

</INLINE>
</FILE>

